{
  "type": "object",
  "title": "createEventRequest",
  "required": [
    "name",
    "startTime",
    "endTime",
    "chainable"
  ],
  "properties": {
    "name": {
      "type": "string",
      "example": "user1"
    },
    "description": {
      "type": "string",
      "example": "user1"
    },
    "startTime": {
      "type": "string",
      "description": "Start time of event, must be in UTC time",
      "example": "user1"
    },
    "endTime": {
      "type": "string",
      "description": "End time of event, must be in UTC time",
      "example": "user1"
    },
    "chainable": {
      "type": "boolean",
      "example": "user1@gmail.com"
    },
    "event_location": {
      "type": "object",
      "title":"eventLocation",
      "required": [
        "name",
        "lat",
        "lon"
      ],
      "properties": {
        "name": {
          "type": "string",
          "example": "user1"
        },
        "lat": {
          "type": "number",
          "example": "user1"
        },
        "lon": {
          "type": "number",
          "example": "user1"
        }
      }
    }
  }
}